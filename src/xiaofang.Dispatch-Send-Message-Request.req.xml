   <session-id></session-id>
   <group-id></group-id>
   <mediamessage>
    <id></id>
    <sender></sender>
    <text></text>
    <picture-uri></picture-uri>
    <audio-uri></audio-uri>
    <audio-length></audio-length>
    <video-uri></video-uri>
    <video-length></video-length>
    <timestamp></timestamp>
   </mediamessage>
  </ns:Dispatch-Send-Message-Request>
 </SOAP-ENV:Body>
</SOAP-ENV:Envelope>
